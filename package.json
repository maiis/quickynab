{
  "name": "quickynab",
  "version": "1.0.7",
  "description": "Quick and easy bank transaction imports to YNAB - supports 116+ banks worldwide",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "ynab": "dist/cli.js"
  },
  "scripts": {
    "dev": "concurrently \"bun run dev:vite\" \"bun run dev:server\"",
    "dev:vite": "vite",
    "dev:server": "bun --watch server.ts",
    "build": "bun run fetch:configs && bun run build:frontend && bun run build:backend && bun run copy:configs",
    "build:frontend": "vite build",
    "build:backend": "bun x tsc --project tsconfig.node.json",
    "fetch:configs": "bun scripts/fetch-bank-configs.ts",
    "copy:configs": "cp lib/parsers/bank2ynab-configs.json dist/lib/parsers/",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "bun test --coverage",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check --write .",
    "prepublishOnly": "bun run check && bun run build",
    "publish": "npm publish",
    "start": "bun dist/cli.js",
    "web": "bun dist/server.js",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "keywords": [
    "ynab",
    "csv",
    "import",
    "cli",
    "bank",
    "transactions",
    "bank2ynab",
    "budgeting"
  ],
  "author": "maiis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maiis/quickynab.git"
  },
  "bugs": {
    "url": "https://github.com/maiis/quickynab/issues"
  },
  "homepage": "https://github.com/maiis/quickynab#readme",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "preferGlobal": true,
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "^14.0.1",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "ynab": "^2.10.0"
  },
  "optionalDependencies": {
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.3.0",
    "fastify": "^5.6.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.9.1",
    "@vitest/coverage-v8": "^4.0.3",
    "@vitest/ui": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json}": "biome check --write --no-errors-on-unmatched"
  }
}
