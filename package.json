{
  "name": "quickynab",
  "version": "1.0.8",
  "description": "Quick and easy bank transaction imports to YNAB - supports 116+ banks worldwide",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "ynab": "dist/cli.js"
  },
  "scripts": {
    "dev": "concurrently \"bun run dev:vite\" \"bun run dev:server\"",
    "dev:vite": "vite",
    "dev:server": "bun --watch server.ts",
    "build": "bun run fetch:configs && bun run build:frontend && bun run build:backend && bun run copy:configs",
    "build:frontend": "vite build",
    "build:backend": "bun x tsc --project tsconfig.node.json",
    "fetch:configs": "bun scripts/fetch-bank-configs.ts",
    "copy:configs": "cp lib/parsers/bank2ynab-configs.json dist/lib/parsers/",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "bun test --coverage",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check --write .",
    "prepublishOnly": "bun run check && bun run build",
    "publish": "npm publish",
    "start": "bun dist/cli.js",
    "web": "bun dist/server.js",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "keywords": [
    "ynab",
    "csv",
    "import",
    "cli",
    "bank",
    "transactions",
    "bank2ynab",
    "budgeting"
  ],
  "author": "maiis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maiis/quickynab.git"
  },
  "bugs": {
    "url": "https://github.com/maiis/quickynab/issues"
  },
  "homepage": "https://github.com/maiis/quickynab#readme",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "preferGlobal": true,
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "14.0.3",
    "csv-parse": "6.1.0",
    "dotenv": "17.2.3",
    "ynab": "2.10.0"
  },
  "optionalDependencies": {
    "@fastify/helmet": "13.0.2",
    "@fastify/multipart": "9.4.0",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/static": "9.0.0",
    "fastify": "5.7.2",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "25.2.0",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "autoprefixer": "10.4.24",
    "concurrently": "9.2.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "node-fetch": "3.3.2",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json}": "biome check --write --no-errors-on-unmatched"
  }
}
