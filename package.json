{
  "name": "quickynab",
  "version": "1.0.0",
  "description": "Quick and easy bank transaction imports to YNAB - supports 116+ banks worldwide",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "ynab": "./dist/cli.js"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:server\"",
    "dev:vite": "vite",
    "dev:server": "tsx watch server.ts",
    "build": "npm run fetch:configs && npm run build:frontend && npm run build:backend && npm run copy:configs",
    "build:frontend": "vite build",
    "build:backend": "tsc --project tsconfig.node.json",
    "fetch:configs": "tsx scripts/fetch-bank-configs.ts",
    "copy:configs": "cp lib/parsers/bank2ynab-configs.json dist/lib/parsers/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "start": "node --no-deprecation dist/cli.js",
    "web": "node --no-deprecation dist/server.js",
    "preview": "vite preview"
  },
  "keywords": [
    "ynab",
    "csv",
    "import",
    "cli",
    "bank",
    "transactions",
    "bank2ynab",
    "budgeting"
  ],
  "author": "",
  "license": "MIT",
  "preferGlobal": true,
  "files": [
    "dist/cli.js",
    "dist/lib/",
    "README.md"
  ],
  "dependencies": {
    "commander": "^14.0.1",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "ynab": "^2.10.0"
  },
  "optionalDependencies": {
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.2.0",
    "fastify": "^5.6.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.6.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
